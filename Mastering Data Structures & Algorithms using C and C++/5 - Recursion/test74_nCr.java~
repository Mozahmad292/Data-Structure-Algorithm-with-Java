public class test74_nCr{

  public static void main(String[]args){
  int n=5; int r=3;
    nCr_iterative(n,r);
  }
  
  static int nCr_iterative(int n,int r){
  return factorial(n)/(factorial(n)*factorial(n-r));
  }
  
    static int factorial(int n){
    if(n==0){
      return 1;
    }else{
      return fact(n-1)*n;
    }
  }
  
  
  
}